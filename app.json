{
  "name": "xray01",
  "description": "将xray部署在Heroku上",
  "keywords": [
    "VMess VLESS"
  ],
  "env": {
    "PROTOCOL": {
      "description": "协议VMess或VLESS，请填小写的vmess或vless",
      "value": "vmess",
      "required": true
    },
    "UUID": {
      "description": "用户的主ID",
      "value": "",
      "required": false
    },
    "WS_PATH": {
      "description": "websocket路径",
      "value": "/ray",
      "required": true
    }
  },
  
  "repository": "https://github.com/yrkdioui/cfsdf ",
  "stack": "container",
  "features": [
    "runtime-dyno-metadata"
  ]
}
